{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A GUI for seekr, a tool for crawling pages",
  "scripts": {
    "build": "tsc",
    "copy-files": "rm -Rf ./dist/pages && mkdir ./dist/pages && cp ./src/pages/* ./dist/pages/",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm run build && npm run copy-files && electron ./dist/main.js"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Internet Computer",
    "seekr"
  ],
  "author": "Ryan Cammer <ryancammer@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "prettier": "2.7.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "electron": "^20.1.3",
    "electron-store": "^8.1.0",
    "eslint": "^8.23.0",
    "prettier": "2.7.1",
    "seekr": "^1.1.10",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "semi": false
  }
}
